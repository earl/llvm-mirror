group("default") {
  deps = [
    "//llvm/lib/Object",
    "//llvm/lib/Target",
    "//llvm/tools/llvm-undname",
    "//llvm/utils/TableGen:llvm-tblgen",
  ]
}

# A pool called "console" in the root BUILD.gn is magic and represents ninja's
# built-in console pool. (Requires a GN with `gn --version` >= 552353.)
pool("console") {
  depth = 1
}
