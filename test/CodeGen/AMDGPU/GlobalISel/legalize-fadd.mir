# RUN: llc -mtriple=amdgcn-mesa-mesa3d -run-pass=legalizer %s -o - | FileCheck %s

---
name:            test_fadd
body: |
  bb.0.entry:
    liveins: $vgpr0, $vgpr1
    ; CHECK-LABEL: name: test_fadd
    ; CHECK: %2:_(s32) = G_FADD %0, %1

    %0:_(s32) = COPY $vgpr0
    %1:_(s32) = COPY $vgpr1
    %2:_(s32) = G_FADD %0, %1
    $vgpr0 = COPY %2
...
